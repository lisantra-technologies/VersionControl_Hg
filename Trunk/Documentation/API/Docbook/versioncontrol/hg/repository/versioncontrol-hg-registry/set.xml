<refentry id="package.versioncontrol.hg.repository.versioncontrol-hg-registry.set">
 <refnamediv>
  <refname>
   <function>VersionControl_Hg_Registry::set</function>
  </refname>
  <refpurpose>Stores an object identified by the name parameter.</refpurpose>
 </refnamediv>
 <refsynopsisdiv>
  <funcsynopsis>
   <funcsynopsisinfo>require_once &apos;/Hg/Registry.php&apos;;</funcsynopsisinfo>
   <funcprototype>
    <funcdef>
     bool
     <function>VersionControl_Hg_Registry::set</function>
    </funcdef>
    <paramdef>
     string
     <parameter>
      <optional>$name = &null;</optional>
     </parameter>
    </paramdef>
    <paramdef>
     mixed
     <parameter>$object</parameter>
    </paramdef>
   </funcprototype>
  </funcsynopsis>
 </refsynopsisdiv>
 <refsect1 id="package.versioncontrol.hg.repository.versioncontrol-hg-registry.set.desc">
  &title.desc;
  &notdocumented;
</refsect1>
 <refsect1 id="package.versioncontrol.hg.repository.versioncontrol-hg-registry.set.param">
  &title.param;
  <para>
   <variablelist>
    <varlistentry>
     <term>
      &type.string;
      <parameter>$name</parameter>
     </term>
     <listitem>
      <para>defaults to $object&apos;s class name</para>
     </listitem>
    </varlistentry>
   </variablelist>
   <variablelist>
    <varlistentry>
     <term>
      &type.mixed;
      <parameter>$object</parameter>
     </term>
     <listitem>
      <para />
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
