<refentry id="package.versioncontrol.hg.versioncontrol-hg-commandproxy.--call">
 <refnamediv>
  <refname>
   <function>VersionControl_Hg_CommandProxy::__call</function>
  </refname>
  <refpurpose>Proxies to the actual implementations of the commands</refpurpose>
 </refnamediv>
 <refsynopsisdiv>
  <funcsynopsis>
   <funcsynopsisinfo>require_once &apos;/Hg/CommandProxy.php&apos;;</funcsynopsisinfo>
   <funcprototype>
    <funcdef>
     <replaceable>
      <link linkend="package.versioncontrol.hg.interfaces.versioncontrol-hg-command-interface-summary">
       <classname>VersionControl_Hg_Command_Interface</classname>
      </link>
     </replaceable>
     <function>VersionControl_Hg_CommandProxy::__call</function>
    </funcdef>
    <paramdef>
     string
     <parameter>$method</parameter>
    </paramdef>
    <paramdef>
     array
     <parameter>
      <optional>$arguments = &null;</optional>
     </parameter>
    </paramdef>
   </funcprototype>
  </funcsynopsis>
 </refsynopsisdiv>
