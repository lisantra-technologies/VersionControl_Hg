     <term>
      &type.string;
      <parameter>$method</parameter>
     </term>
     <listitem>
      <para>is the command to instantiate</para>
     </listitem>
    </varlistentry>
   </variablelist>
   <variablelist>
    <varlistentry>
     <term>
      &type.array;
      <parameter>$arguments</parameter>
     </term>
     <listitem>
      <para>for the command</para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
